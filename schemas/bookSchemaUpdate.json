{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "book"
    ],
    "properties": {
        "book": {
            "type": "object",
            "default": {},
            "title": "The book Schema",
            "required": [
                "isbn",
                "amazon_url",
                "author",
                "language",
                "pages",
                "publisher",
                "title",
                "year"
            ],
            "properties": {
                "isbn": {
                    "type": "string",
                    "default": "",
                    "title": "The isbn Schema",
                    "examples": [
                        "0691161518"
                    ]
                },
                "amazon_url": {
                    "type": "string",
                    "default": "",
                    "title": "The amazon_url Schema",
                    "examples": [
                        "https://taco.com"
                    ]
                },
                "author": {
                    "type": "string",
                    "default": "",
                    "title": "The author Schema",
                    "examples": [
                        "mctest"
                    ]
                },
                "language": {
                    "type": "string",
                    "default": "",
                    "title": "The language Schema",
                    "examples": [
                        "english"
                    ]
                },
                "pages": {
                    "type": "integer",
                    "default": 0,
                    "title": "The pages Schema",
                    "examples": [
                        1000
                    ]
                },
                "publisher": {
                    "type": "string",
                    "default": "",
                    "title": "The publisher Schema",
                    "examples": [
                        "yeah right"
                    ]
                },
                "title": {
                    "type": "string",
                    "default": "",
                    "title": "The title Schema",
                    "examples": [
                        "UPDATED BOOK"
                    ]
                },
                "year": {
                    "type": "integer",
                    "default": 0,
                    "title": "The year Schema",
                    "examples": [
                        2000
                    ]
                }
            },
            "examples": [
                {
                    "isbn": "0691161518",
                    "amazon_url": "https://taco.com",
                    "author": "mctest",
                    "language": "english",
                    "pages": 1000,
                    "publisher": "yeah right",
                    "title": "UPDATED BOOK",
                    "year": 2000
                }
            ]
        }
    },
    "examples": [
        {
            "book": {
                "isbn": "0691161518",
                "amazon_url": "https://taco.com",
                "author": "mctest",
                "language": "english",
                "pages": 1000,
                "publisher": "yeah right",
                "title": "UPDATED BOOK",
                "year": 2000
            }
        }
    ]
}